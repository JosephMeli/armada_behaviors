<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Navigation">

	<executable package_path="armada_flexbe_behaviors.navigation_sm" class="NavigationSM" />
	<tagstring></tagstring>
	<author>Kevin Yassini</author>
	<date>Tue Feb 22 2022</date>
	<description>
		Move the robot to a defined location from RVIZ.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="x" default="0" label="x" hint="Sets the x">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="numeric" name="y" default="0" label="y" hint="Sets the y">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="numeric" name="theta" default="0" label="theta" hint="Sets the theta">
			<min value="0" />
			<max value="1" />
		</param>

	</params>


</behavior>